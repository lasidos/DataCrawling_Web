@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.BodyClass = "mtcSubNotice";
}

@section Header{
    @RenderPage("~/Views/Shared/_Header.cshtml", Model)
    <link href="~/Resource/Css/Help/Common.css" rel="stylesheet" />
    <link href="~/Resource/Css/Help/_Help.css" rel="stylesheet" />
}

@section Notice{
    @RenderPage("~/Views/Shared/_Notice.cshtml")
}

@section Content{
    <div id="content">
        <div class="container">
            <div class="content-article">
                @RenderPage("~/Views/Shared/Left/_Help.cshtml")
                <div class="loungeContent">
                    <div class="topHdWrap clear">
                        <h2 class="lug_hd_2">문의·신고</h2>
                    </div>
                    <div class="faqWrap">
                        <div class="mtcTplTab">
                            <ul class="tabItems">
                                <li class="on" data-tab="1"><a href="javascript:;">문의·신고</a></li>
                                <li class="" data-tab="2"><a href="javascript:;">문의·신고 내역</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>
        $(function () {
            $('.mtcTplTab .tabItems li').click(function () {
                $('.mtcTplTab .tabItems li').removeClass('on');
                $(this).addClass('on');
            });
        });
    </script>
}